<style>



    .evidenceSection ._box {
        position: relative;
    }

    ._box label  {
        display: block;
        letter-spacing: 3px !important;
        font-size: 1.5rem !important;
        font-weight: 700 !important;
        text-transform: uppercase;
        color: #607D8B;
        border: none !important;
        margin:0 0 0 0 !important;
        vertical-align: middle;
        padding: 0 !important;
        height:25px !important;

    }

    input {
        max-width: none;
    }

    .boxchecklist label {
        height: 53px !important;
    }


        ._box input [type=text] {
            font-size: 1rem;
            color: #455A64;
            display: inline !important;
            width: 100% !important;
            height: 3rem !important;
            padding: 0 1rem !important;
            border: 0.125rem solid #CFD8DC !important;
            border-radius: 0.25rem !important;
            margin-bottom: 1rem;
            transition: 0.5s all;
        }

    .rounded {
        border-radius: 0.25rem !important;
    }



        ._box input.newTag:focus, ._box input:active {
            outline: none;
            border: 0.125rem solid #654ea3;
            box-shadow: 0 0 0.5rem rgba(101,78,163,0.15);
            
        }

    .evidenceSection ul {
        display: block;
        padding: 0;
        margin: 0;
    }

    ._box ul::after {
        content: "";
        clear: both;
        display: table;
    }

    ._box ul li {
        position: relative;
        list-style: none;
        float: left;
        font-size: 1.5rem;
        text-transform: capitalize;
        background-color: rgba(101,78,163,0.7);
        color:white;
        line-height: 2rem;
        padding: 0.8rem 3.5rem 0.8rem 1.2rem;
        border-radius: 1rem;
        margin-right: 0.5rem;
        margin-bottom: 0.5rem;
        word-break: break-all;
    }

        ._box ul li > span.rmTag {
            position: absolute;
            margin: 0.25rem 0.25rem 1rem 0.25rem;
            top: 3px;
            right: 0;
            height: 2.5rem;
            width: 2.5rem;
            border-radius: 1rem;
            color: #fff;
            font-size: 2.5rem;
            line-height: 3rem;
            text-align: center;
            background-color: #654ea3;
            cursor: pointer;
            -webkit-transition: 0.3s all;
            transition: 0.3s all;
        }

            ._box ul li > span.rmTag:hover {
                background-color: #ef9a9a;
            }
        .w-100 {
            width:100% !important;
        }

    .border-0 {
        border: 0 !important;
    }
</style>

