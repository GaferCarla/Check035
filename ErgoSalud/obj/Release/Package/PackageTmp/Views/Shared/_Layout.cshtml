    <style>
        #footer {
            position: fixed;
            height: 50px;
            background-color: red;
            bottom: 0px;
            left: 0px;
            right: 0px;
            margin-bottom: 0px;
        }
        /* navbar bg */
        .bg-light {
            background: linear-gradient(90deg, rgba(92, 23, 230, 0.55) 0%, rgba(9, 9, 121, 1) 50%, rgba(255, 87, 87, 055) 100%) !important;
        }

        a, a:active, a:visited, a:focus, a:hover {
            display: inline-block;
            text-decoration: none;
            color: white;
        }

        .disabled {
            pointer-events: none;
            cursor: not-allowed;
        }

        body {
            /*background-image: url(/images/resta.jpg);*/
        }

        #order {
            display: none;
        }

        #Current_orders {
            display: none;
        }

        .contenedores_div {
            background-color: white;
            border: 2px solid darkorange;
            outline-style: double;
            outline-color: black;
        }

        .titulos {
            color: darkorange;
            font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif
        }
    </style>
