@model ErgoSalud.Models.E360_Cuestionario_Resultado_N01
@{
    ViewBag.Title = "Evaluación 360";
    List<SelectListItem> listItems = new List<SelectListItem>();
    listItems.Add(new SelectListItem
    {
        Text = "Sí mismo",
        Value = "Sí mismo"
    });
    listItems.Add(new SelectListItem
    {
        Text = "Compañero",
        Value = "Compañero",
    });
    listItems.Add(new SelectListItem
    {
        Text = "Superior",
        Value = "Superior"
    });
}
@if (TempData["Saving_Edit_Message"] != null)
{
    <script type="text/javascript">
        swal.fire({
            title: "CUESTIONARIO COMPLETADO Y ENVIADO.",
            text: "Gracias por su participación",
            type: "success", showConfirmButton: false, timer: 2000
        });

        window.location.href = '/Home/Index';
    </script>
}
<style>
    .br_col-xs {
        display: none;
    }


    @@media only screen and (max-width : 1199px) {
        .wrap_pregunta {
            word-wrap: break-word;
        }

        .br_col-xs {
            display: block;
        }

        .wrap_respuesta {
            word-wrap: normal;
        }
    }

    @@media (min-width : 1200px) and (max-width: 9000px) {
        .wrap_pregunta {
            white-space: nowrap;
        }
    }

    input[type='radio']:after {
        width: 14px;
        height: 14px;
        position: relative;
        float: left;
        background-color: #d1d3d1;
        content: '';
        display: inline-block;
        visibility: visible;
        border: 1.5px solid whitesmoke;
    }



    input[type='radio']:checked:after {
        width: 15px;
        height: 15px;
        top: -2px;
        left: -1px;
        position: relative;
        background-color: BLACK;
        content: '';
        display: inline-block;
        visibility: visible;
        border: 1px solid whitesmoke;
    }
</style>

