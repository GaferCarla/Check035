@model ErgoSalud.Models.ERGOS_Usuarios_N01

@{
    ViewBag.Title = "Delete";
}


<h3 style="color:red">REALMENTE QUIERE ELIMINAR ESTE USUARIO?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Nombre de Usuario")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.User_Nombre)
        </dd>
        <dt>
            @Html.DisplayName("Email")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.email)
        </dd>

        <dt>
            @Html.DisplayName("Empresa")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ERGOS_Empresas_N01.Razon_Social)
        </dd>

        <dt>
            @Html.DisplayName("Rol Asignado")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ERGOS_Roles_N01.Nombre_Rol)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-danger" /> 
            @Html.ActionLink("Cancelar", "Index",null, new { @class="btn btn-primary"})
        </div>
    }
</div>
